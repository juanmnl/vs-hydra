{
  "name": "Hydra Plus",
  "type": "dark",
  "colors": {
    "activityBar.background": "#1e1f2d",
    "activityBar.foreground": "#d3d5da",
    "activityBarBadge.background": "#FF407B",
    "activityBarBadge.foreground": "#D7DAE0",
    "button.background": "#FF407B",
    "button.foreground": "#000000",
    "button.hoverBackground": "#41bfff",
    "diffEditor.insertedTextBackground": "#00809B33",
    "dropdown.background": "#353b45",
    "dropdown.border": "#181A1F",
    "editor.background": "#2a2c3c",
    "editor.foreground": "#ffe08c",
    "editorBracketMatch.border": "#FF407B",
    "editor.lineHighlightBackground": "#2C313A",
    "editor.selectionBackground": "#3E4451",
    "editorCursor.foreground": "#ffe08c",
    "editorGroup.border": "#181A1F",
    "editorGroupHeader.tabsBackground": "#242533",
    "editorIndentGuide.background": "#3C4049",
    "editorLineNumber.foreground": "#5C6370",
    "editorWhitespace.foreground": "#3C4049",
    "editorHoverWidget.background": "#242533",
    "editorHoverWidget.border": "#181A1F",
    "editorSuggestWidget.background": "#242533",
    "editorSuggestWidget.border": "#181A1F",
    "editorSuggestWidget.selectedBackground": "#2C313A",
    "editorWidget.background": "#21252B",
    "input.background": "#1B1D23",
    "input.border": "#181A1F",
    "focusBorder": "#FF407B",
    "list.activeSelectionBackground": "#2C313A",
    "list.activeSelectionForeground": "#D7DAE0",
    "list.focusBackground": "#2C313A",
    "list.hoverBackground": "#2C313A",
    "list.highlightForeground": "#D7DAE0",
    "list.inactiveSelectionBackground": "#2C313A",
    "list.inactiveSelectionForeground": "#D7DAE0",
    "notification.background": "#21252B",
    "pickerGroup.border": "#ff407b",
    "scrollbarSlider.background": "#4E566680",
    "scrollbarSlider.activeBackground": "#747D9180",
    "scrollbarSlider.hoverBackground": "#5A637580",
    "sideBar.background": "#242533",
    "sideBarSectionHeader.background": "#242533",
    "statusBar.background": "#242533",
    "statusBar.foreground": "#9DA5B4",
    "statusBar.debuggingBackground": "#838CC4",
    "statusBar.debuggingForeground": "#FFFFFF",
    "statusBarItem.hoverBackground": "#2C313A",
    "statusBar.noFolderBackground": "#242533",
    "tab.activeBackground": "#2a2c3c",
    "tab.border": "#181A1F",
    "tab.inactiveBackground": "#242533",
    "titleBar.activeBackground": "#242533",
    "titleBar.activeForeground": "#9DA5B4",
    "titleBar.inactiveBackground": "#242533",
    "titleBar.inactiveForeground": "#3C4049",
    "extensionButton.prominentBackground": "#FF407B",
    "extensionButton.prominentHoverBackground": "#ff407b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Punctuation within templates",
      "scope": "string.template meta.embedded.line",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language", "variable.other.object.js"],
      "settings": {
        "foreground": "#41BFFF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#96A1FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage (declaration or modifier keyword)",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "String Quoted Content",
      "scope": [
        "string.quoted.single.js",
        "string.quoted.double.js",
        "string.quoted.double.json",
        "string.quoted.double.xml",
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.single.css",
        "string.quoted.double.css"
      ],
      "settings": {
        "foreground": "#D7DAE0"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "Language constant (boolean, null)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Entity",
      "scope": "entity.name",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "HTML or XML tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "HTML or XML tag brackets",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Tag atttribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#F2F2F2"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "variable.function"],
      "settings": {
        "foreground": "#ffe08c"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Control keyword (if, try, while, etc.)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Special operator",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "Support function",
      "scope": "support.function",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Support variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "Object literal key / property",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "JS Variable Property",
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Key-value separator",
      "scope": "punctuation.separator.key-value",
      "settings": {
        "foreground": "#FF0064"
      }
    },
    {
      "name": "Embedded puncuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "Puncuation Definition block",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ffe08c"
      }
    },
    {
      "name": "Template expression",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#FF407B"
      }
    },
    {
      "name": "CSS property",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css"
      ],
      "settings": {
        "foreground": "#F2F2F2"
      }
    },
    {
      "name": "Color",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Font names",
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "CSS #id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#ffe08c"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#F2F2F2"
      }
    },
    {
      "name": "CSS support functions (rgb)",
      "scope": "support.function.misc.css",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "Markup quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FF407B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "Markup link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#F2F2F2"
      }
    },
    {
      "name": "Markup link description",
      "scope": "string.other.link.description",
      "settings": {
        "foreground": "#F08484"
      }
    },
    {
      "name": "Python function call",
      "scope": "meta.function-call.generic.python",
      "settings": {
        "foreground": "#ffe08c"
      }
    },
    {
      "name": "C# storage type",
      "scope": "storage.type.cs",
      "settings": {
        "foreground": "#41BFFF"
      }
    },
    {
      "name": "C# local variable",
      "scope": "entity.name.variable.local.cs",
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "C# properties and fields",
      "scope": [
        "entity.name.variable.field.cs",
        "entity.name.variable.property.cs"
      ],
      "settings": {
        "foreground": "#96A1FF"
      }
    },
    {
      "name": "C++ operators",
      "scope": "source.cpp keyword.operator",
      "settings": {
        "foreground": "#FF407B"
      }
    }
  ]
}
